{
    "openapi":"3.0.2",
    "info": {
      "title":"Spring boot template",
      "version":"1.0"
    },
    "paths": {
      "/api/v1/healthcheck": {
        "get": {
          "description": "Healthcheck endpoint",
          "responses": {
            "200": {
              "description": "OK"
            }
          }
        }
      },
      "/api/v1/account": {
        "get": {
          "description": "Get all accounts from DB",
          "responses": {
            "200": {
              "description": "A list of accounts",
              "content": {
                "application/json" : {
                  "schema" :{
                    "type": "array",
                    "items" :{
                      "type": "object",
                      "properties": {
                        "id" :{
                          "type" : "string",
                          "format": "uuid"
                        },
                        "email" :{
                          "type": "string"
                        },
                        "username" : {
                          "type": "string"
                        },
                        "status" : {
                          "type": "string"
                        },
                        "create_at" : {
                          "type": "string",
                          "format": "time"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }