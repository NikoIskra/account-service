{
  "openapi": "3.0.2",
  "info": {
    "title": "Spring boot template",
    "version": "1.0"
  },
  "paths": {
    "/api/v1/healthcheck": {
      "get": {
        "tags": [
          "healthcheck"
        ],
        "description": "Healthcheck endpoint",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/v1/account": {
      "post": {
        "tags": [
          "account"
        ],
        "description": "Creates new account and stores it into database",
        "requestBody": {
          "description": "account to be added",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "contentSchema/accountCommand.json"
              }
            }
          }
        },
        "responses": {
          "201": {
            "$ref": "responses/201Created.json"
          },
          "400": {
            "$ref": "responses/400BadRequest.json"
          },
          "409": {
            "$ref": "responses/409Conflict.json"
          },
          "default": {
            "$ref": "responses/default.json"
          }
        }
      }
    }
  }
}